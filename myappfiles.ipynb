   "source": [
    "txt = input(\"Enter your sentence: \")\n",
    "txt = txt.split(\"_\")\n",
    "new_list = []\n",
    "for text in txt:\n",
    "    lower_txt = text.lower()\n",
    "    new_list.append(lower_txt)\n",
    "new_sentence = ' '.join(new_list)\n",
    "print(new_sentence)"
   ]
